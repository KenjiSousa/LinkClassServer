{
  "name": "linkclassserver",
  "version": "1.0.0",
  "description": "Projeto para servidor em Node para o LinkClass.",
  "scripts": {
    "dev": "npm run format && npm run build & tsx --env-file .env.dev src/main.ts",
    "start": "md logs 2> nul & node --env-file .env.prod dist/main.js > \"logs/log%date:~-4%-%date:~3,2%-%date:~0,2% %time:~0,2%-%time:~3,2%-%time:~6,2%.txt\"",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "Guilherme Antunes, Kenji Sousa e Rafael Miranda",
  "type": "module",
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "google-auth-library": "^10.3.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mysql": "^2.15.27",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "imports": {
    "#*": "./dist/*"
  }
}
